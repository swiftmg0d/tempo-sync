# Port number on which the application server will listen for incoming connections
# Example: PORT=3000
PORT=

# API key for X integration
# Used for accessing X API services
X_API_KEY=

# Environment mode (development, production, test)
# Example: NODE_ENV=development
NODE_ENV=

# Spotify API credentials for music data integration
# Spotify application client ID
CLIENT_ID=
# Spotify application client secret
CLIENT_SECRET=
# Redirect URI specifically for Spotify OAuth flow
REDIRECT_URI=
# Spotify API permissions scope requested from the provider
SCOPE=


# Strava API credentials for fitness data integration
# Strava application client ID
STRAVA_CLIENT_ID=
# Strava application client secret
STRAVA_CLIENT_SECRET=
# Redirect URI specifically for Strava OAuth flow
REDIRECT_URI_STRAVA=

# Database connection string
# Example: DATABASE_URL=postgresql://user:password@localhost:5432/dbname
DATABASE_URL=

# Secret key used for session encryption and security
# Should be a long, random string in production
SESSION_SECRET=

# Application encryption key
# Used for additional data encryption/decryption
KEY=
