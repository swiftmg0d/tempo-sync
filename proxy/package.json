{
	"name": "proxy",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"deploy": "wrangler deploy",
		"dev": "wrangler dev --ip 0.0.0.0 --port 8787",
		"start": "wrangler dev",
		"test": "vitest",
		"cf-typegen": "wrangler types",
		"docker:build": "docker build -t tempo-sync-proxy -f .Dockerfile .",
		"docker:run": "docker run -p 8787:8787 --env-file .env tempo-sync-proxy",
		"docker:dev": "pnpm docker:build && pnpm docker:run"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.8.19",
		"@eslint/js": "^9.39.1",
		"@types/compression": "^1.8.1",
		"@types/eslint__js": "^9.14.0",
		"eslint": "^9.39.1",
		"eslint-plugin-perfectionist": "^4.15.1",
		"eslint-plugin-prettier": "^5.5.4",
		"typescript": "^5.5.2",
		"typescript-eslint": "^8.48.0",
		"vitest": "~3.2.0",
		"wrangler": "^4.51.0"
	},
	"dependencies": {
		"compression": "^1.8.1",
		"dotenv": "^17.2.3",
		"eslint-config-prettier": "^10.1.8",
		"helmet": "^8.1.0",
		"hono": "^4.10.7",
		"jiti": "^2.6.1",
		"morgan": "^1.10.1"
	}
}
